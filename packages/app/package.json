{
  "name": "@browse/client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --port 8080",
    "lint": "next lint",
    "pkg": "pkg .",
    "start": "PORT=8080 node server.js"
  },
  "bin": "server.js",
  "pkg": {
    "compress": "GZip",
    "outputPath": "build",
    "assets": [
      ".next/**/*",
      "next.config.js"
    ],
    "scripts": [
      ".next/server/**/*.js"
    ],
    "targets": [
      "node18-macos-arm64",
      "node18-win-x64"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "dotenv": "^16.0.3",
    "keyboardjs": "^2.7.0",
    "localforage": "^1.10.0",
    "mime-types": "^2.1.35",
    "next": "^13.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/keyboardjs": "^2.5.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@types/sql.js": "^1.4.4",
    "@types/sqlite3": "^3.1.8",
    "autoprefixer": "^10.4.13",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "mini-css-extract-plugin": "^2.7.5",
    "pkg": "^5.5.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9"
  }
}
